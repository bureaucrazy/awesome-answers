<h1>All Questions</h1>
<% @questions.each do |q| %>
  <%# These two do the same as the third line %>
  <%# <%= link_to q.title, question_path({id: q.id}) %>
  <%# <%= link_to q.title, q %>
  <div>
    <% text = "#{q.id}. #{q.title} (#{q.view_count})" %>
    <%= link_to( text, question_path(q) ) %>
  </div>
<% end %>
